    script syrc;
    lookup ltrFina;
} fina;

# HB PAUSE
#---  medi feature (pass 6) ---
feature medi {
    # Medial Forms
    script syrc;
    lookup ltrMedi;
} medi;

# HB PAUSE
#---  init feature (pass 7) ---
feature init {
    # Initial Forms
    script syrc;
    lookup ltrInit;
} init;

# HB PAUSE
# I don't know what to do with these 3 features
feature fin2 {
    # Terminal Forms
    script syrc;
    lookup ltrFinaSerOlaph;
} fin2;

feature fin3 {
    # Terminal Forms
    script syrc;
    lookup ltrFinaESyOlaph;
} fin3;

feature med2 {
    # Medial Forms
    script syrc;
    lookup ltrMediOlaph;
} med2;

# HB PAUSE
#---  rlig feature (pass 8) ---
feature rlig {
    # Ligatures  -- we use this because liga is done in the same pass as cvXX
    script syrc;
    lookup GRishSeyame;
    lookup taw_alaph;
} rlig;

# feature rclt - nothing to do
