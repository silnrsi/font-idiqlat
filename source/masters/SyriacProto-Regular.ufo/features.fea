# Lookups for user-selectable features
#NB: Lookup names ending in "TT" are used only by TypeTuner
#**********************************
#  GSUB FEATURES
#**********************************
# These are intended to be arranged in the order MS & HB engines execute.
#---  rtlm feature (pass 2) ---
# feature rtlm {  # Right-To-Left Mirrored Forms
#     lookup Mirror;
# } rtlm ;
# HB PAUSE
#---  ccmp feature (pass 3) ---
feature ccmp {
    # Glyph Composition/Decomposition
    #    script latn;
    #        lookup LatinPunctuation;
    script syrc;
    lookup GPreDecompose;
    lookup ccmp_syrc_zz02_syrczz01_1;
    lookup r1;
    lookup r2;
    lookup RukkakhaSquare;
} ccmp;

# HB PAUSE
#---  locl feature (pass 3) not used ---
# HB PAUSE
#---  fina feature (pass 5) ---
feature fina {
    # Terminal Forms
    script syrc;
    lookup ltrFina;
} fina;

# HB PAUSE
#---  medi feature (pass 6) ---
feature medi {
    # Medial Forms
    script syrc;
    lookup ltrMedi;
} medi;

# HB PAUSE
#---  init feature (pass 7) ---
feature init {
    # Initial Forms
    script syrc;
    lookup ltrInit;
} init;

# HB PAUSE
