    # Terminal Forms
    script syrc;
    lookup ltrFinaSerOlaph;
} fin2;

feature fin3 {
    # Terminal Forms
    script syrc;
    lookup ltrFinaESyOlaph;
} fin3;

feature med2 {
    # Medial Forms
