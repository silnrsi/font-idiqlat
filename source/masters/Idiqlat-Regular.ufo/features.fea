    # Same for latin & syriac:
    lookup MarkToBase;
    lookup LMarkOnLigaEast;
} mark;

feature mkmk {
    # Mark to mark Positioning
    # Same for latin & syriac:
    lookup MarkToMarkAbove;
    lookup MarkToMarkBelow;
} mkmk;
