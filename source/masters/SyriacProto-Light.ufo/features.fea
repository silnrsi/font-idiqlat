    lookupflag IgnoreMarks;
    sub alaph-syriac by alaph-syriac.medi;
} ltrMediOlaph;

lookup ltrFinaSerOlaph {
    lookupflag IgnoreMarks;
    sub alaph-syriac by alaph-syriac.fina.alt2;
